#################################################################
#    Object Properties
#################################################################

###  https://pokemonkg.org/ontology#containsPlace
:containsPlace rdf:type owl:ObjectProperty ;
               owl:inverseOf :locatedIn ;
               rdf:type owl:AsymmetricProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain :Place ;
               rdfs:range :Place ;
               rdfs:comment "A place is contained to some extent in this place."@en ,
                            "Ein Ort ist in gewissem Maße in diesem Ort enthalten."@de ;
               rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
               rdfs:label "beinhaltet Ort"@de ,
                          "contains place"@en .


###  https://pokemonkg.org/ontology#describedInPokédex
:describedInPokédex rdf:type owl:ObjectProperty ;
                    owl:inverseOf :describesPokémon ;
                    rdf:type owl:InverseFunctionalProperty ;
                    rdfs:domain :Species ;
                    rdfs:range :PokédexEntry ;
                    rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                    rdfs:label "described in Pokédex entry"@en ,
                               "in Pokédex Eintrag beschrieben"@de .


###  https://pokemonkg.org/ontology#describesPokémon
:describesPokémon rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :PokédexEntry ;
                  rdfs:range :Species ;
                  rdfs:comment "A Pokémon that is described by this Pokédex entry."@en ,
                               "Ein Pokémon, das von diesem Pokédex Eintrag beschrieben wird."@de ;
                  rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                  rdfs:label "beschreibt Pokémon"@de ,
                             "describes Pokémon"@en .


###  https://pokemonkg.org/ontology#evolvesFrom
:evolvesFrom rdf:type owl:ObjectProperty ;
             owl:inverseOf :evolvesTo ;
             rdfs:domain :Species ;
             rdfs:range :Species ;
             rdfs:comment "Diese Spezie entwickelt sich von einer anderen Spezies."@de ,
                          "This species evolves from the other species."@en ;
             rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
             rdfs:label "entwickelt sich von"@de ,
                        "evolves from"@en .


###  https://pokemonkg.org/ontology#evolvesTo
:evolvesTo rdf:type owl:ObjectProperty ;
           rdfs:domain :Species ;
           rdfs:range :Species ;
           rdfs:comment "Diese Spezie kann sich in eine andere Spezie entwickeln."@de ,
                        "This species can evolve in another species."@en ;
           rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
           rdfs:label "entwickelt sich in"@de ,
                      "evolves to"@en .


###  https://pokemonkg.org/ontology#featuresSpecies
:featuresSpecies rdf:type owl:ObjectProperty ;
                 rdfs:range :Species ;
                 owl:propertyChainAxiom ( :hasPokédexEntry
                                          :describesPokémon
                                        ) ;
                 rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                 rdfs:label "features Species"@en .


###  https://pokemonkg.org/ontology#foundIn
:foundIn rdf:type owl:ObjectProperty ;
         rdfs:domain :Species ;
         rdfs:range :Habitat ;
         rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
         rdfs:label "found in"@en ,
                    "kommt vor in"@de .


###  https://pokemonkg.org/ontology#hasColour
:hasColour rdf:type owl:ObjectProperty ;
           rdfs:range <http://dbpedia.org/ontology/Colour> ;
           rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
           rdfs:label "has colour"@en ,
                      "hat Farbe"@de .


###  https://pokemonkg.org/ontology#hasFlavor
:hasFlavor rdf:type owl:ObjectProperty ;
           rdfs:domain :Food ;
           rdfs:range :Flavor ;
           rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
           rdfs:label "has flavor"@en ,
                      "hat Geschmacksrichtung"@de .


###  https://pokemonkg.org/ontology#hasPokédexEntry
:hasPokédexEntry rdf:type owl:ObjectProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain :Pokédex ;
                 rdfs:range :PokédexEntry ;
                 rdfs:comment "A Pokédex entry in which this Pokémon is described."@en ,
                              "Ein Pokédex Eintrag in welchen über dieses Pokémon informiert wird."@de ;
                 rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                 rdfs:label "has Pokédex entry"@en ,
                            "hat Pokédex Eintrag"@de .


###  https://pokemonkg.org/ontology#hasShape
:hasShape rdf:type owl:ObjectProperty ;
          rdfs:domain :Species ;
          rdfs:range :Shape ;
          rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
          rdfs:label "has shape"@en ,
                     "hat Silhouette"@de .


###  https://pokemonkg.org/ontology#hasSize
:hasSize rdf:type owl:ObjectProperty ;
         rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
         rdfs:label "has size"@en ,
                    "hat Größe"@de .


###  https://pokemonkg.org/ontology#hasType
:hasType rdf:type owl:ObjectProperty ;
         rdfs:range :Type ;
         rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
         rdfs:label "has Type"@en ,
                    "hat Typ"@de .


###  https://pokemonkg.org/ontology#inEggGroup
:inEggGroup rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Species ;
            rdfs:range :EggGroup ;
            rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
            rdfs:label "in Ei-Gruppe"@de ,
                       "in egg group"@en .


###  https://pokemonkg.org/ontology#learnsMove
:learnsMove rdf:type owl:ObjectProperty ;
            rdfs:domain :MoveLearning ;
            rdfs:range :Move ;
            rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
            rdfs:label "learns move"@en ,
                       "lernt Aktion"@de .


###  https://pokemonkg.org/ontology#locatedIn
:locatedIn rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain :Place ;
           rdfs:range :Place ;
           rdfs:comment "Dieser Ort befindet sich in einem gewissen Maße in einem anderen Ort."@de ,
                        "This place is located to some extent in another place."@en ;
           rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
           rdfs:label "enthalten in"@de ,
                      "located in"@en .


###  https://pokemonkg.org/ontology#mayHaveAbility
:mayHaveAbility rdf:type owl:ObjectProperty ;
                rdfs:domain :Species ;
                rdfs:range :Ability ;
                rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                rdfs:label "könnte Fähigkeit besitzen"@de ,
                           "may have ability"@en .


###  https://pokemonkg.org/ontology#mayHaveHiddenAbility
:mayHaveHiddenAbility rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :mayHaveAbility ;
                      rdfs:domain :Species ;
                      rdfs:range :Ability ;
                      rdfs:label <https://pokemonkg.org/ontology> ,
                                 "könnte versteckte Fähigkeit haben"@de ,
                                 "may have hidden ability"@en .


#################################################################
#    Data properties
#################################################################

###  https://pokemonkg.org/ontology#accuracy
:accuracy rdf:type owl:DatatypeProperty ;
          rdfs:domain :Move ;
          rdfs:range xsd:integer ;
          rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
          rdfs:label "has accuracy"@en ,
                     "hat Genauigkeit"@de .


###  https://pokemonkg.org/ontology#basePower
:basePower rdf:type owl:DatatypeProperty ;
           rdfs:domain :Move ;
           rdfs:range xsd:integer ;
           rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
           rdfs:label "has base power"@en ,
                      "hat Basisstärke"@de .


###  https://pokemonkg.org/ontology#basePowerPoints
:basePowerPoints rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :Move ;
                 rdfs:range xsd:integer ;
                 rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                 rdfs:label "has base power points"@en ,
                            "hat Basis-Angriffspunkte"@de .


###  https://pokemonkg.org/ontology#effectDescription
:effectDescription rdf:type owl:DatatypeProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Ability
                                               :Move
                                             )
                               ] ;
                   rdfs:label "effect description"@en .


###  https://pokemonkg.org/ontology#entryNumber
:entryNumber rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :PokédexEntry ;
             rdfs:range xsd:int ;
             rdfs:comment "Der Pokédex Eintrag hat folgende Nummer, die eindeutig sein muss."@de ,
                          "This Pokédex entry has the given number, which has to be unique."@en ;
             rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
             rdfs:label "has entry number"@en ,
                        "hat Eintragsnummer"@de .


###  https://pokemonkg.org/ontology#firmness
:firmness rdf:type owl:DatatypeProperty ;
          rdfs:domain :Food ;
          rdfs:range xsd:integer ;
          rdfs:label "has firmness"@en ,
                     "hat Güte"@de .


###  https://pokemonkg.org/ontology#hasGenus
:hasGenus rdf:type owl:DatatypeProperty ;
          rdfs:domain :Species ;
          rdfs:label "has genus"@en ,
                     "hat Gattung"@de .


###  https://pokemonkg.org/ontology#maxPowerPoints
:maxPowerPoints rdf:type owl:DatatypeProperty ;
                rdfs:domain :Move ;
                rdfs:range xsd:integer ;
                rdfs:label <https://pokemonkg.org/ontology> ,
                           "has maximum power points"@en ,
                           "hat maximale Agriffspunkte"@de .


###  https://pokemonkg.org/ontology#minLevelToLearn
:minLevelToLearn rdf:type owl:DatatypeProperty ;
                 rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                 rdfs:label "braucht mininum Level zum Erlernen"@de ,
                            "needs minimum level to learn"@en .


###  https://pokemonkg.org/ontology#smoothness
:smoothness rdf:type owl:DatatypeProperty ;
            rdfs:domain :Food ;
            rdfs:range xsd:integer ;
            rdfs:label "has smoothness"@en ,
                       "hat Glätte"@de .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Colour
<http://dbpedia.org/ontology/Colour> rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Person
<http://xmlns.com/foaf/0.1/Person> rdf:type owl:Class .


###  https://pokemonkg.org/ontology#Ability
:Ability rdf:type owl:Class ;
         owl:disjointUnionOf ( :Flavor
                               :Generation
                               :Item
                             ) ;
         rdfs:comment "Abilities were introduced in Generation III as an all new game mechanic. Each and every Pokémon has an ability, and can only have one at a time. Some abilities are exclusive to certain Pokémon and Evolution lines, while others are known by many Pokémon."@en ,
                      "Fähigkeiten wurden in Generation III als völlig neue Spielmechanik eingeführt. Jeder einzelne Pokémon hat eine Fähigkeit und kann nur eine auf einmal haben. Einige Fähigkeiten sind exklusiv für bestimmte Pokémon- und Evolutionslinien, während andere von vielen Pokémon bekannt sind."@de ;
         rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
         rdfs:label "Ability"@en ,
                    "Fähigkeit"@de .


###  https://pokemonkg.org/ontology#BattleItem
:BattleItem rdf:type owl:Class ;
            rdfs:subClassOf :Item ;
            rdfs:isDefinedBy <https://pokemonkg.org/ontology> .


###  https://pokemonkg.org/ontology#Berry
:Berry rdf:type owl:Class ;
       rdfs:subClassOf :Food ;
       rdfs:comment "Beeren sind kleine, saftige, fleischige Früchte. Wie in der realen Welt gibt es auch in der Pokémon-Welt eine große Vielfalt mit einer großen Auswahl an Geschmacksrichtungen, Namen und Effekten. Erstmals in den Spielen der Generation II gefunden, sind viele Beeren inzwischen zu kritischen Gegenständen im Kampf geworden, wo ihre verschiedenen Effekte die Wiederherstellung von HP und Statuszustand, Statikverbesserung und sogar Schadensverneinung beinhalten."@de ,
                    "Berries are small, juicy, fleshy fruit. As in the real world, a large variety exists in the Pokémon world, with a large range of flavors, names, and effects. First found in the Generation II games, many Berries have since become critical held items in battle, where their various effects include HP and status condition restoration, stat enhancement, and even damage negation."@en ;
       rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
       rdfs:label "Beere"@de ,
                  "Berry"@en .


###  https://pokemonkg.org/ontology#EggGroup
:EggGroup rdf:type owl:Class ;
          rdfs:comment "Egg group is a category that determines which Pokémon are able to interbreed. The concept was introduced in Generation II, along with breeding. Similar to types, a Pokémon may belong to either one or two egg groups."@en ;
          rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
          rdfs:label "Egg Group"@en ,
                     "Ei-Gruppe"@de .


###  https://pokemonkg.org/ontology#Flavor
:Flavor rdf:type owl:Class ;
        rdfs:comment "Flavor is a special set of attributes that certain foods in the Pokémon world have. Most of the foods can have more than one flavor attributed to them."@en ,
                     "Geschmacksrichtung ist ein spezielles Attribut für Nahrung in der Pokémon-Welt. Den meisten Nahrungen können mehr als eine Geschmacksrichtung haben."@de ;
        rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
        rdfs:label "Flavor"@en ,
                   "Geschmacksrichtung"@de .


###  https://pokemonkg.org/ontology#Food
:Food rdf:type owl:Class ;
      rdfs:subClassOf :Item ;
      rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
      rdfs:label "Food"@en ,
                 "Nahrung"@de .


###  https://pokemonkg.org/ontology#Game
:Game rdf:type owl:Class ;
      rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
      rdfs:label "Game"@en ,
                 "Spiel"@de .


###  https://pokemonkg.org/ontology#Generation
:Generation rdf:type owl:Class ;
            rdfs:comment "Generation bezieht sich auf die Pokémon-Spieleserie. Es ist eine Gruppe von Spielen, die zur gleichen Zeit oder etwa zur gleichen Zeit veröffentlicht wurden. Es bedeutet auch, dass Spiele in der gleichen Generation mit den anderen kompatibel sind, die den gleichen Pokémon enthalten und die Anzahl der Züge, die dort gelernt werden müssen."@de ,
                         "Generation refers to the Pokémon game series. It is a group of games that were released at or around the same time. It also means that games in the same generation are compatible with the others, containing the same Pokémon and the number of moves there are to be learned."@en ;
            rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
            rdfs:label "Generation"@de ,
                       "Generation"@en .


###  https://pokemonkg.org/ontology#Gym
:Gym rdf:type owl:Class ;
     rdfs:subClassOf :Place ;
     rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
     rdfs:label "Arena"@de ,
                "Gym"@en .


###  https://pokemonkg.org/ontology#GymLeader
:GymLeader rdf:type owl:Class ;
           rdfs:subClassOf :Trainer ;
           rdfs:comment "A gym leader is the highest-ranking member and owner of an official Pokémon Gym. Gym leaders use their gyms and their Pokémon to test the skills of trainers that challenge them, and if said trainers win a battle, the gym leader will gift them with a badge that's unique to that specific gym."@en ;
           rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
           rdfs:label "Arenaleiter"@de ,
                      "Gym Leader"@en .


###  https://pokemonkg.org/ontology#HM
:HM rdf:type owl:Class ;
    rdfs:subClassOf :Item ,
                    :MoveLearning ;
    rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
    rdfs:label "Hidden Machine"@en ,
               "Versteckte Maschine"@de .


###  https://pokemonkg.org/ontology#Habitat
:Habitat rdf:type owl:Class ;
         rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
         rdfs:label "Habitat"@en ,
                    "Lebensraum"@de .


###  https://pokemonkg.org/ontology#HoldItem
:HoldItem rdf:type owl:Class ;
          rdfs:subClassOf :Item ;
          rdfs:isDefinedBy <https://pokemonkg.org/ontology> .


###  https://pokemonkg.org/ontology#Item
:Item rdf:type owl:Class ;
      rdfs:comment "An item is an object in the Pokémon games which the player can pick up, keep in their Bag, and use in some manner. They have various uses, including healing, powering up, helping one to catch Pokémon, or to access a new area."@en ,
                   "Ein Gegenstand ist ein Objekt in den Pokémon-Spielen, welches der Spieler aufheben, in seiner Tasche aufbewahren und in irgendeiner Weise verwenden kann. Sie haben verschiedene Verwendungsmöglichkeiten, einschließlich Heilung, Auftanken, Hilfe beim Fangen von Pokémon oder beim Betreten eines neuen Gebietes."@de ;
      rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
      rdfs:label "Item"@de ,
                 "Item"@en ;
      <http://www.w3.org/2004/02/skos/core#altLabel> "Gegenstand"@de .


###  https://pokemonkg.org/ontology#LearningByLevelingUp
:LearningByLevelingUp rdf:type owl:Class ;
                      rdfs:subClassOf :MoveLearning ;
                      rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                      rdfs:label "Erlernen durch Aufleveln"@de ,
                                 "Learning by leveling up"@en .


###  https://pokemonkg.org/ontology#LearningThroughBreeding
:LearningThroughBreeding rdf:type owl:Class ;
                         rdfs:subClassOf :MoveLearning ;
                         rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
                         rdfs:label "Erlernen durch Züchtung"@de ,
                                    "Learning through breeding"@en .


###  https://pokemonkg.org/ontology#Medicine
:Medicine rdf:type owl:Class ;
          rdfs:subClassOf :Item ;
          rdfs:comment "Medicine items can heal various afflictions of a Pokémon."@en ,
                       "Medizin kann verschiedene Leiden eines Pokémon heilen."@de ;
          rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
          rdfs:label "Medicine"@en ,
                     "Medizin"@de .


###  https://pokemonkg.org/ontology#Move
:Move rdf:type owl:Class ;
      owl:disjointUnionOf ( :Flavor
                            :Game
                            :Generation
                            :Item
                            :Place
                            :Pokédex
                            :PokédexEntry
                            :Pokémon
                            :Region
                            :Type
                          ) ;
      rdfs:comment "A Move is a technique that a Pokémon uses during Battles. Moves are mainly used to inflict damage on the opponent. Moves usually come from a natural ability that the specific Pokémon has. Some moves can be used both inside and outside of battle. When outside of battle, moves are usually used for transportation or for removing a certain obstacle that the player runs into in the game."@en ,
                   "Eine Aktion ist eine Technik, die ein Pokémon während eines Kampfes anwendet. Züge werden hauptsächlich verwendet, um dem Gegner Schaden zuzufügen. Züge kommen normalerweise von einer natürlichen Fähigkeit, die der bestimmte Pokémon hat. Einige Züge können sowohl innerhalb als auch außerhalb des Kampfes verwendet werden. Außerhalb des Kampfes werden Züge normalerweise für den Transport oder zum Entfernen eines bestimmten Hindernisses verwendet, auf das der Spieler im Spiel stößt."@de ;
      rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
      rdfs:label "Aktion"@de ,
                 "Move"@en .


###  https://pokemonkg.org/ontology#MoveLearning
:MoveLearning rdf:type owl:Class ;
              rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
              rdfs:label "Aktionserlernen"@de ,
                         "Move learning"@en .


###  https://pokemonkg.org/ontology#PhysicalMove
:PhysicalMove rdf:type owl:Class ;
              rdfs:subClassOf :Move ;
              rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
              rdfs:label "Physical Move"@en ,
                         "Physische Aktion"@de .


###  https://pokemonkg.org/ontology#Place
:Place rdf:type owl:Class ;
       rdfs:comment "Entities that have a somewhat fixed, physical extension."@en ,
                    "Entitäten, die eine einigermaßen feste, physische Ausdehnung haben."@de ;
       rdfs:isDefinedBy <https://pokemonkg.org/ontology> ;
       rdfs:label "Ort"@de ,
                  "Place"@en .


###  https://pokemonkg.org/ontology#Pokéball
:Pokéball rdf:type owl:Class ;
          rdfs:subClassOf :Item ;
          rdfs:comment "A Poké Ball is a type of item that is critical to a Trainer's quest, used for catching and storing Pokémon."@en ,
                       "Ein Poké-Ball ist eine Art von Gegenstand, der für die Aufgabe eines Trainers entscheidend ist und zum Fangen und Aufbewahren von Pokémon verwendet wird."@de ;
